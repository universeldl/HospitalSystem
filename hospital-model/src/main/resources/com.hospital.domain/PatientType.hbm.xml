<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hospital.domain.PatientType" dynamic-insert="true" dynamic-update="true">
        <id name="patientTypeId" type="java.lang.Integer">
            <column name="patientTypeId"/>
            <generator class="identity"/>
        </id>

        <property name="patientTypeName" type="java.lang.String">
            <column name="patientTypeName" unique="true"/>
        </property>




    </class>
</hibernate-mapping>
